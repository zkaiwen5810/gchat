ifeq ($(origin CC), default)
  CC = clang
endif
ifeq ($(origin CXX), default)
  CXX = clang++
endif
ifeq ($(origin AR), default)
  AR = ar
endif
INCLUDES += -I../App-Common/Source -I../Walnut/vendor/glm -I../Walnut/Walnut/Source -I../Walnut/Walnut/Platform/Headless -I../Walnut/vendor/spdlog/include -I../Walnut/vendor/yaml-cpp/include -I../Walnut/Walnut-Modules/Walnut-Networking/Source -I../Walnut/Walnut-Modules/Walnut-Networking/vendor/GameNetworkingSockets/include
